{
  "name": "@eggjs/cluster",
  "version": "3.0.0-beta.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "cluster manager for egg",
  "main": "./dist/commonjs/index.js",
  "scripts": {
    "lint": "eslint --cache src test --ext .ts",
    "pretest": "npm run lint -- --fix && npm run prepublishOnly",
    "test": "egg-bin test",
    "preci": "npm run lint && npm run prepublishOnly",
    "ci": "egg-bin cov && attw --pack",
    "prepublishOnly": "tshy && tshy-after"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eggjs/cluster.git"
  },
  "keywords": [
    "egg",
    "cluster",
    "process"
  ],
  "author": "dead-horse <dead_horse@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eggjs/cluster/issues"
  },
  "homepage": "https://github.com/eggjs/cluster#readme",
  "dependencies": {
    "@eggjs/utils": "^4.0.2",
    "@fengmk2/ps-tree": "^2.0.1",
    "cfork": "^2.0.0",
    "cluster-reload": "^2.0.0",
    "detect-port": "^2.0.1",
    "egg-logger": "^3.6.0",
    "get-ready": "^3.2.0",
    "graceful-process": "^2.0.0",
    "sendmessage": "^3.0.1",
    "terminal-link": "^2.1.1",
    "utility": "^2.2.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.1",
    "@eggjs/tsconfig": "1",
    "@types/mocha": "10",
    "@types/node": "22",
    "address": "^2.0.3",
    "coffee": "^5.5.1",
    "egg": "beta",
    "egg-bin": "6",
    "egg-errors": "^2.2.0",
    "egg-mock": "beta",
    "eslint": "8",
    "eslint-config-egg": "14",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.1",
    "tshy": "3",
    "tshy-after": "1",
    "typescript": "5",
    "urllib": "^4.6.8"
  },
  "engines": {
    "node": ">= 18.19.0"
  },
  "type": "module",
  "tshy": {
    "exports": {
      ".": "./src/index.ts",
      "./package.json": "./package.json"
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/commonjs/index.d.ts",
  "module": "./dist/esm/index.js"
}
